local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("crucifix guiE", "bloodTheme")
local Tab = Window:NewTab("crucifix")
local Section = Tab:NewSection("CRUCIFIX e")
Section:NewButton("crucifix", "Gives banned crucifix", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ZepsyyCodesLUA/Utilities/main/Crucifix"))()
end)
